        <%-include('../main/header')%>
        <body>
            <div>
              <%-include('./sideIndex')%>
            </div>
        <div class="card shadow mb-4">
            <div class="card-header py-3">
                <h6 class="m-0 font-weight-bold text-primary">공지사항</h6>
            </div>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                        <thead>
                            <tr>
                                <th>글번호</th>
                                <th>제목</th>
                                <th>작성자</th>
                                <th>작성일</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% for(let i = 0; i < data.length; i++){ %>
                                <tr>
                                    <td><%= data[i].id%></td>
                                    <td> <a href="/notice/<%= data[i].id%>"><%= data[i].subject%> </a> </td>
                                    <td><%= data[i].user_dept%>과<%= data[i].user_position%> <%= data[i].user_name%></td>
                                    <td><%= data[i].createdAt%></td>
                                </tr>                                
                            <% } %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

    </body>
        <%-include('../main/footer')%>
